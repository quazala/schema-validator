{
  "name": "@quazala/schema-validator",
  "version": "0.0.1",
  "description": "Quazala schema-validator library",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest run",
    "prepare": "pnpm run build",
    "check": "pnpm biome check --write ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quazala/schema-validator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quazala/schema-validator/issues"
  },
  "homepage": "https://github.com/quazala/schema-validator#readme",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "^20.16.2",
    "lefthook": "^1.7.14",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
